<script>
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
    });
    $(document).on('click', '.btnXemChiTiet', function (e) {
       var MaPhieuKham = $(this).attr('Id');
        $.ajax({
            url: "@Url.Action("DsPhieuKham", "NhanVienNhanBenh")",
            type: "GET",

        dataType: "json",

        success: function (data, textStatus, jqXHR) {
            $.each(data, function (i, item) {
                if (item.MaPhieuKham == MaPhieuKham)
                {
                    $.ajax({
                        url: "@Url.Action("DsUser", "NhanVienNhanBenh")",
                        type: "GET",

                    dataType: "json",

                    success: function (data, textStatus, jqXHR) {
                        $.each(data, function (i, item1) {
                            if (item1.MaBenhNhan == item.MaBenhNhan)
                            {
                                $("#Tuoibn").val(item1.Tuoi);
                                $("#TenBenhNhan").val(item1.HoTenBenhNhan);

                                $("#DiaChibn").val(item1.DiaChi);
                                $("#SDTbn").val(item1.SDT);

                            }
                        });

                    }

                });
                    $("#SoThuTuKham").val(item.SoThuTu);
                    $.ajax({
                        url: "@Url.Action("DsBacSi", "NhanVienNhanBenh")",
                        type: "GET",

                    dataType: "json",

                    success: function (data, textStatus, jqXHR) {
                        $.each(data, function (i, item2) {
                            if (item2.MaBacSi == item.MaBacSi)
                            {

                                $("#BacSiKham").val(item2.TenBacSi);

                            }
                        });

                    }

                });
                    $("#BacSi").val(item.MaBacSi);
                    $("#TrangThai").val(item.TrangThai);


                }
            });

        },
        error: function (jqXHR, textStatus, errorThrown) {
            console.log(textStatus);
        }
    });
    $("#ChiTietModal").modal('show');
    });
    //

</script>

