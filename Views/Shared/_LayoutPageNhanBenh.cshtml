<script>
    $(document).ready(function () {
        $(document).on('click', '.btncapso', function (e) {

            var $row = $(this).parent().parent().children();
            var mabn = $row.find("div[id=mabn]").html();

            $.ajax({
                url: "http://localhost:46814/NhanVienNhanBenh/kiemtra/" + mabn.toString(),
                type: "GET",

                success: function (data, textStatus, jqXHR) {

                    if (data ==0 ) {
                        document.getElementById("alertt").innerHTML = "Bệnh nhân đã khám trong ngày";
                        return;
                    }
                    else {
                        document.getElementById("alertt").innerHTML = "";
                        location.href = "http://localhost:46814/NhanVienNhanBenh/CapSoKhamBenh/"+ mabn;
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    console.log(textStatus);
                }
            });
        });





        $(document).on('click', '.btnXemChiTietbs', function (e) {

            var id = document.getElementById("bacsi").value;
            $.ajax({
                url: "@Url.Action("DsBacSi", "NhanVienNhanBenh")",
                type: "GET",
            dataType: "json",
            success: function (data, textStatus, jqXHR) {
                $.each(data, function (i, item1) {
                    if (item1.MaBacSi == id) {
                        $("#TenBacSi").val(item1.TenBacSi);
                        $("#DiaChiBacSi").val(item1.Diachi);

                        $("#PhongKham").val(item1.Phongkham);
                        $("#SDTBacSi").val(item1.SDTBS);
                        $("#TrinhDoBacSi").val(item1.TrinhDo);

                        $("#ChuyenMon").val(item1.ChuyenMon);
                    }


                });

            }

        });

        $("#ChiTietBsModal").modal('show');
    });

    });


